# dependencies
/node_modules
/.pnp
.pnp.*
.yarn/*
!.yarn/patches
!.yarn/plugins
!.yarn/releases
!.yarn/versions

# testing
/coverage

# next.js
/.next/
/out/

# production
/build

# misc
.DS_Store
*.pem

# debug
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*

# env files (can opt-in for committing if needed)
.env*
# this line ignores all env files, including .env.local, .env.development.local, .env.test.local, .env.production.local

# But i want to commit .env.example file
!/.env.example  # this line negates the previous line for .env.example file, so it will not be ignored and can be committed to the repository.
# vercel
.vercel
LEARNING.md
# here LEARNING.md is ignored because it contains sensitive information
